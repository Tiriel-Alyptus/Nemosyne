{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/[...all].js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
